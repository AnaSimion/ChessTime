@model ChessProject.Models.ContactInfo
@{
    ViewBag.Name = "Update ContactInfo";
}

<h2>@ViewBag.Name</h2>

@using (Html.BeginForm(actionName: "Edit", controllerName: "ContactInfo", method: FormMethod.Post))
{
    @Html.HttpMethodOverride(HttpVerbs.Put)
    @Html.HiddenFor(b => b.ContactInfoId)
  

    @Html.LabelFor(bt => bt.PhoneNumber, "PhoneNumber:")
    <br />
    @Html.EditorFor(bt => bt.PhoneNumber, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(bt => bt.PhoneNumber, "", new { @class = "text-danger" })
    <br /><br />

        @Html.LabelFor(bt => bt.Email, "Email:")
    <br />
    @Html.EditorFor(bt => bt.Email, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(bt => bt.Email, "", new { @class = "text-danger" })
    <br /><br />

    <button class="btn btn-primary" type="submit">Update</button>
}